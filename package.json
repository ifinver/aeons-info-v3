{
	"name": "aeons-info",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build-content": "node src/build-content.js",
		"build-css": "npx tailwindcss -i ./src/build-styles.css -o ./src/client/styles.css --watch",
