		"cf-typegen": "wrangler types"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"@tailwindcss/typography": "^0.5.16",
		"autoprefixer": "^10.4.21",
		"marked": "^16.2.1",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.33.1"
	}
